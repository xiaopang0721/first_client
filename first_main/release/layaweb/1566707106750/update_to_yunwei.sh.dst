#!/bin/bash

__root=`pwd`
for p in conf/*.dst; do
	if [[ $p =~ "config.js.dst" ]]; then
		continue;
	fi
	cp -rf $p ../first_client_yunwei/first_release/conf/
	echo "拷贝 $p"
done
#cp -rf conf/*.dst ../first_client_yunwei/first_release/conf
rm -rf ../first_client_yunwei/first_release/rsync_folder
mkdir ../first_client_yunwei/first_release/rsync_folder
cp -rf rsync_folder ../first_client_yunwei/first_release/
echo "拷贝 rsync_folder"
cp -rf game_data.bin ../first_client_yunwei/first_release/
echo "拷贝 game_data.bin"
cp -rf main.js ../first_client_yunwei/first_release/
echo "拷贝 main.js"
cp -rf minfo.json ../first_client_yunwei/first_release/
echo "拷贝 minfo.json"
cp -rf libs/all_libs.min.js ../first_client_yunwei/first_release/libs
echo "拷贝 libs/all_libs.min.js"
cp -rf libs/worker.js ../first_client_yunwei/first_release/libs
echo "拷贝 libs/worker.js"
cp -rf yunwei.index.html.dst ../first_client_yunwei/first_release/index.html
echo "拷贝 index.html"
cp -rf yunwei.index.demo.html.dst ../first_client_yunwei/first_release/index_debug.html
echo "拷贝 index_debug.html"
cp -rf version_h5.bin ../first_client_yunwei/first_release/version_h5.bin
echo "拷贝 version_h5.bin"
cp -rf version_h5.txt ../first_client_yunwei/first_release/version_h5.txt
echo "拷贝 version_h5.txt"
cp -rf version_h5_min.bin ../first_client_yunwei/first_release/version_h5_min.bin
echo "拷贝 version_h5_min.bin"
cp -rf version_h5_min.txt ../first_client_yunwei/first_release/version_h5_min.txt
echo "拷贝 version_h5_min.txt"
cp -rf main.min.js ../first_client_yunwei/first_release/main.min.js
echo "拷贝 main.min.js"